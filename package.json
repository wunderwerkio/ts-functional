{
  "name": "@wunderwerk/ts-functional",
  "version": "1.0.0-beta.2",
  "type": "module",
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.1",
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "@swc/core": "^1.3.51",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@wunderwerk/eslint-config": "^1.0.3",
    "ava": "^5.2.0",
    "c8": "^7.13.0",
    "esbuild": "^0.17.17",
    "eslint": "^8.38.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^41.1.2",
    "husky": "^8.0.3",
    "prettier": "^2.8.7",
    "rollup": "^3.20.4",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "ts-node": "^10.9.1",
    "tsd": "^0.28.1",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint --ext .ts src --max-warnings 0",
    "format:check": "prettier --check '**/*.{ts,tsx}'",
    "format:write": "prettier --write '**/*.{ts,tsx}'",
    "test": "NODE_NO_WARNINGS=1 ava && tsd -t dist/results/index.d.ts --files tests/**/*.test-d.ts",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "check-all": "pnpm run build && pnpm run '/(lint|test|typecheck)/'",
    "version-package": "pnpm run check-all && changeset version",
    "publish-package": "pnpm run check-all && changeset publish",
    "prepare": "husky install"
  },
  "ava": {
    "files": [
      "tests/**/*.test.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader",
      "ts-node/esm"
    ]
  },
  "exports": {
    "./results": {
      "types": "./dist/results/index.d.ts",
      "import": "./dist/results/index.mjs"
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ]
}
